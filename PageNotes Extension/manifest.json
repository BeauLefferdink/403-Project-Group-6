{
    "manifest_version": 3,
    "name": "PageNotes",
    "version": "1.0",
    "description": "Take and save notes for different web pages",
    "permissions": ["storage", "tabs"],
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
    }
    },
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
    }
    }