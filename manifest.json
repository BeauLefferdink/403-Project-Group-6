{
  "manifest_version": 2,
  "name": "Note Manager",
  "version": "1.0",
  "description": "A Chrome extension for managing notes.",
  "permissions": [],
  "browser_action": {
    "default_popup": "welcome.html",
    "default_icon": {
      "16": "images/icon.png"
